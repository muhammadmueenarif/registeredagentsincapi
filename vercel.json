{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "CORPTOOLS_ACCESS_KEY": "@corptools-access-key",
    "CORPTOOLS_SECRET_KEY": "@corptools-secret-key"
  }
}
